{
  "name": "nest-drizzle",
  "private": true,
  "packageManager": "pnpm@9.15.1",
  "description": "",
  "type": "module",
  "license": "MIT",
  "author": "Innei <https://innei.in>",
  "scripts": {
    "build": "npm run build:packages && pnpm -C \"apps/core\" run build",
    "build:packages": "sh ./scripts/pre-build.sh",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "dev": "pnpm -C \"apps/core\" run start",
    "dev:web": "pnpm -C \"apps/web\" run dev",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint --fix",
    "prebuild": "rimraf dist",
    "predev": "npm run build:packages",
    "prepare": "if [ \"$NODE_ENV\" = \"production\" ]; then echo 'skip prepare in production' ;else corepack enable && simple-git-hooks; fi",
    "pretest": "npm run predev",
    "test": "pnpm -C \"test\" run test"
  },
  "dependencies": {
    "cross-env": "7.0.3",
    "lodash": "4.17.21"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.21.0",
    "@innei/bump-version": "^1.5.10",
    "@innei/prettier": "^0.15.0",
    "@nestjs/cli": "10.4.8",
    "@nestjs/schematics": "10.2.3",
    "@sxzz/eslint-config": "4.5.1",
    "concurrently": "9.1.0",
    "dotenv-cli": "7.4.4",
    "drizzle-kit": "0.30.1",
    "eslint": "9.17.0",
    "eslint-plugin-react-hooks": "5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "fastify": "^4.29.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.10",
    "prettier": "3.4.2",
    "rimraf": "6.0.1",
    "simple-git-hooks": "2.11.1",
    "ts-loader": "9.5.1",
    "tsconfig-paths": "4.2.0",
    "tsup": "8.3.5",
    "tsx": "4.19.2",
    "typescript": "^5.7.2",
    "zx": "8.2.4"
  },
  "resolutions": {
    "*>lodash": "4.17.21",
    "*>typescript": "^5.2.2",
    "pino": "./external/pino"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --ignore-path ./.prettierignore --write "
    ],
    "*.{js,ts,cjs,mjs,jsx,tsx,json}": [
      "eslint --fix"
    ]
  },
  "bump": {
    "before": [
      "git pull --rebase"
    ]
  },
  "redisMemoryServer": {
    "downloadDir": "./tmp/redis/binaries",
    "version": "6.0.10",
    "disablePostinstall": "1",
    "systemBinary": "/opt/homebrew/bin/redis-server"
  }
}
